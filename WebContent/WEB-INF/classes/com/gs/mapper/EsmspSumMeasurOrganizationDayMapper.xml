    <select id="selectTop5" resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from esmsp_sum_measur_organization_day
		order by sum_value LIMIT 5
	</select>
	<select id="selectByCode" parameterType="java.lang.String"
		resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from 
		<choose>
		 	<when test="date!=null and date.length>0">
                  esmsp_sum_measur_organization_day
            </when>
            <otherwise>
                  esmsp_sum_measur_organization_day_temp
            </otherwise>
         </choose>
		where 1=1
		 and euo_code=#{euoCode}
		 <if test="date!=null and date.length>0">
		and date_code in
		<foreach collection="date" item="d" open="(" separator="," close=")">  
            #{d}  
        </foreach> 
        </if>
		and mmp_code in
		 <foreach collection="mmpCodes" item="mmpCode" open="(" separator="," close=")">  
            #{mmpCode}  
        </foreach>
        <if	test="eusCode!=null and eusCode!=''">
        	and eus_code=#{eusCode}
        </if>
	</select>
	<select id="daySum" parameterType="java.lang.String"
		resultType="java.lang.Double">
		select
		sum(sum_value) from
		<choose>
		 	<when test="dateCode!=null and dateCode!=''">
                  esmsp_sum_measur_organization_day
            </when>
            <otherwise>
                  esmsp_sum_measur_organization_day_temp
            </otherwise>
         </choose>
		where 1=1
		<if test="ctds!=null and ctds.length>0">
			and eus_code in
			<foreach collection="ctds" item="ctd" open="(" separator="," close=")">  
            	#{ctd}  
       	 	</foreach> 
		</if>
		<if test="dateCode!=null and dateCode!=''">
			and date_Code=#{dateCode}
		</if>
		<if test="mmpCode!=null and mmpCode.length>0">
			and mmp_code in
			<foreach collection="mmpCode" item="m" open="(" separator="," close=")">  
            	#{m} 
       	 	</foreach> 
		</if>
		
	</select>
	<select id="lyearSum" parameterType="java.lang.String"
		resultType="java.lang.Double">
		select
		last_value from
                  esmsp_sum_measur_organization_day_temp
		where 1=1
			and eus_code =
            	#{ctds}  
		<if test="mmpCodes!=null and mmpCodes.length>0">
			and mmp_code in
			<foreach collection="mmpCodes" item="m" open="(" separator="," close=")">  
            	#{m} 
       	 	</foreach> 
		</if>
		
	</select>
